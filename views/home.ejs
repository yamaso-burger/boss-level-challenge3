<%- include("partials/header") %>
<h1 class="title">Home</h1>
<p><%= content %></p>
<% posts.forEach((post)=>{ %>
    <div class="post">
        <h2><%= post.title %></h2>
        <p class="post-content"><%= post.content.substr(0, 100) + " ..." %><span><a href="/posts/<%= post.title %>">Read More</a></span></p>
    </div> 
<% }); %>

<%- include("partials/footer") %>